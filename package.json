{
  "name": "manage-locks-and-keys",
  "version": "1.0.0",
  "description": "Locks and keys, Roskilde Festival",
  "web": "src/App.js",
  "scripts": {
    "build": "rm -Rf build/app.*.js && rm -Rf build/app.*.js.map && cross-env NODE_ENV=production webpack -p --progress",
    "deploy": "cross-env NODE_ENV=production npm run build && deploy/bundle-for-firebase-deploy.sh",
    "dev": "npm run dev-server",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --port 8082 --colors --content-base build/ --progress --watch-poll",
    "dev-local": "webpack -d --watch --progress --colors",
    "lint": "eslint src/",
    "test": "wdio ./wdio.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rfit/locks-and-keys"
  },
  "author": "RFIT // Martin Holm Jensen",
  "license": "-",
  "bugs": {
    "url": "https://github.com/rfit/locks-and-keys/issues"
  },
  "homepage": "https://github.com/rfit/locks-and-keys/rf-voting#readme",
  "dependencies": {
    "babel-es6-polyfill": "^1.1.0",
    "browser-sync": "^2.15.0",
    "css-loader": "^0.25.0",
    "eslint-plugin-promise": "^2.0.1",
    "fetch-jsonp": "^1.0.3",
    "firebase": "^3.6.4",
    "isomorphic-fetch": "2.2.1",
    "material-ui": "^0.16.4",
    "react": "^15.3.0",
    "react-css-modules": "^3.7.10",
    "react-dom": "^15.3.0",
    "react-facebook-login": "^3.2.0",
    "react-fontawesome": "^1.1.0",
    "react-paginate": "^4.0.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-toggle-display": "^2.1.1",
    "react-virtualized": "^8.11.2",
    "redux": "^3.6.0",
    "redux-form": "^6.2.0",
    "redux-logger": "2.6.1",
    "redux-react-firebase": "^2.5.0",
    "redux-thunk": "^2.1.0",
    "resolve-url": "^0.2.1",
    "resolve-url-loader": "^1.6.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-react": "^6.11.1",
    "bootstrap-loader": "^1.3.0",
    "browser-sync": "^2.12.8",
    "browser-sync-webpack-plugin": "^1.1.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "cross-env": "^2.0.1",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-jsx": "^3.0.0",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "firebase-tools": "^3.2.1",
    "flow-bin": "^0.35.0",
    "font-awesome-loader": "^1.0.0",
    "mocha": "^3.2.0",
    "node-sass": "^3.9.3",
    "nodemon": "^1.10.0",
    "parallelshell": "^2.0.0",
    "react-addons-update": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.0.2",
    "url-loader": "^0.5.7",
    "wdio-browserstack-service": "^0.1.4",
    "wdio-mocha-framework": "^0.5.8",
    "wdio-selenium-standalone-service": "0.0.8",
    "wdio-spec-reporter": "0.0.5",
    "webdriverio": "^4.6.2",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "node5"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "impliedStrict": true,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": {
      "Headers": true,
      "fetch": true
    },
    "rules": {
      "no-confusing-arrow": 0,
      "react/jsx-quotes": 0,
      "no-multi-spaces": [
        1,
        {
          "exceptions": {
            "VariableDeclarator": true,
            "ImportDeclaration": true
          }
        }
      ]
    }
  }
}
